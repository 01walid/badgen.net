{
  "hosting": [ {
    "target": "badgen",
    "public": "dist",
    "ignore": [
      "**/*"
    ],
    "rewrites": [ {
      "source": "**",
      "run": {
        "serviceId": "badgen",
        "region": "us-central1"
      }
    } ]
  }, {
    "target": "flat-badgen",
    "public": "dist",
    "ignore": [
      "**/*"
    ],
    "rewrites": [ {
      "source": "**",
      "run": {
        "serviceId": "badgen-flat",
        "region": "us-central1"
      }
    } ]
  } ]
}
